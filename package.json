{
  "name": "monomodel",
  "version": "0.1.0",
  "private": true,
  "author": "Benjamin Gudehus <hastebrot@gmail.com>",
  "scripts": {
    "site": "craco start",
    "site:build": "craco build",
    "site:test": "craco test --verbose=false",
    "guide": "start-storybook -p 9001 -c .storybook/ --ci",
    "guide:build": "build-storybook -c .storybook/ -o ./build/monomodel-guide",
    "fix": "prettier \"{src,.storybook}/**/*.{js,jsx,json,md,mdx}\" --write"
  },
  "dependencies": {
    "@cloudflare/json-schema-walker": "^0.1.1",
    "classnames": "^2.2.6",
    "fannypack": "^4.14.2",
    "immer": "^2.1.5",
    "lodash-es": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-beautiful-dnd": "^11.0.0-beta.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@craco/craco": "^4.1.0",
    "@storybook/addon-options": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@types/node": "^11.13.4",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "prettier": "^1.16.4",
    "react-hooks-testing-library": "^0.4.0",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": false,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  }
